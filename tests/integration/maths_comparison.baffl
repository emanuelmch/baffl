fun lessThan(x: i32, y: i32): bool {
    return x < y;
}

fun lessThanOrEqualTo(x: i32, y: i32): bool {
    return x <= y;
}

fun main(): i32 {
    var result = 0;

    if (lessThan(1, 2)) {
        result = result + 1;
    }

    if (lessThan(2, 2)) {
        result = result + 2;
    }

    if (lessThanOrEqualTo(1, 2)) {
        result = result + 4;
    }

    if (lessThanOrEqualTo(2, 2)) {
        result = result + 8;
    }

    return result;
}
